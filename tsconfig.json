{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@Components/*": ["./src/components/*"],
      "@Layouts/*": ["./src/layouts/*"],
      "@Pages/*": ["./src/pages/*"],
      "@Styles/*": ["./src/styles/*"],
      "@Configs/*": ["./src/configs/*"],
      "@Stores/*": ["./src/stores/*"],
      "@Hooks/*": ["./src/hooks/*"],
      "@Assets/*": ["./src/assets/*"],
      "@AppTypes/*": ["./src/types/*"],
      "@Data/*": ["./src/data/*"]
    }
  }
}
